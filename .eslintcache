   setSearchResult(undefined)\n      setSeek(null)\n    }\n  }, [search])\n  \n  useEffect(() => {\n    setPageCount(Math.ceil(listOfUsers.length / usersPerPage))\n  }, [usersPerPage, listOfUsers]);\n \n  // useEffect(() => {\n  //   setUsersPerPage(usersPerPage);\n  // }, []);\n\n  const loading = async () => {\n    try {\n      const res = await axios.get(nextUrl)\n      await dispatch(addUserThunk(res.data))\n      if (res.data.length > 0) {\n        setNextUrl(res.headers.nexturl)\n        console.log(res)\n        return\n      }\n      setSeek(null)\n    } catch (error) {\n      console.log(error)\n    }\n  }\n\n\n  const handleSearchButton = async () => {\n    //busca na API com o nome salvo no estado search e altera o estado searchResult;\n    setSeek(true)\n    await setSearchResult(\n      listOfUsers.filter((user) => user.name.includes(search))\n    )\n    setSeek(null)\n  }\n\n  const handleSetUsersPerPage = (evt) => {\n    setUsersPerPage(evt.target.value);\n  };\n\n  const handleSearchInput = (ev) => {\n    // pegar valor digitado no input\n    setSearch(ev.target.value)\n  }\n  const handleSetPage = (evt, value) => {\n    history.push(`/list/page=${value}`);\n  };\n\n\n  return (\n    <>\n      <input\n        value={search}\n        onChange={handleSearchInput}\n        placeholder=\"pesquisar usuário\"\n      />\n      <button onClick={() => handleSearchButton()}>Search</button>\n\n\n      <UsersPageContainer>\n\n      {seek ? (\n        <div>Loading...</div>\n      ) : seek === null ? (\n        searchResult ? (\n          <UsersFind searchResult={searchResult} />\n        ) : (\n          <UsersMap listOfUsers={listOfUsers} usersPerPage={usersPerPage} page = {page} />\n        )\n      ) : (\n        <div>No match found</div>\n      )}\n\n        <PaginationContainer>\n        <Pagination\n          count={pageCount}\n          onChange={handleSetPage}\n          page={page}\n          size=\"small\"\n        />\n        <Select onChange={handleSetUsersPerPage} value={usersPerPage}>\n          <MenuItem value={10}>10/Page</MenuItem>\n          <MenuItem value={20}>20/Page</MenuItem>\n          <MenuItem value={50}>50/Page</MenuItem>\n          <MenuItem value={100}>100/Page</MenuItem>\n        </Select>\n      </PaginationContainer>\n\n      </UsersPageContainer>\n    </>\n  )\n}\n\nexport default List\n","/home/joao/atividades/atividades-a-fazer/kenziehub/src/pages/profile/index.jsx",["140","141"],"/home/joao/atividades/atividades-a-fazer/kenziehub/src/pages/signIn/index.jsx",[],"/home/joao/atividades/atividades-a-fazer/kenziehub/src/pages/signUp/index.jsx",[],"/home/joao/atividades/atividades-a-fazer/kenziehub/src/store/modules/listOfUsers/actions.js",[],"/home/joao/atividades/atividades-a-fazer/kenziehub/src/components/listComponent-UsersMap/index.jsx",[],"/home/joao/atividades/atividades-a-fazer/kenziehub/src/components/ButtonRedirect/index.jsx",[],"/home/joao/atividades/atividades-a-fazer/kenziehub/src/components/login/index.jsx",[],"/home/joao/atividades/atividades-a-fazer/kenziehub/src/components/listComponent-UsersFind/index.jsx",[],"/home/joao/atividades/atividades-a-fazer/kenziehub/src/components/ProfilePreferences/index.jsx",[],["142","143"],"/home/joao/atividades/atividades-a-fazer/kenziehub/src/components/Techs/index.jsx",[],"/home/joao/atividades/atividades-a-fazer/kenziehub/src/components/Works/index.jsx",[],"/home/joao/atividades/atividades-a-fazer/kenziehub/src/pages/profile/styles.js",[],"/home/joao/atividades/atividades-a-fazer/kenziehub/src/store/modules/validationToken/reducer.js",[],"/home/joao/atividades/atividades-a-fazer/kenziehub/src/store/modules/validationToken/actionType.js",[],"/home/joao/atividades/atividades-a-fazer/kenziehub/src/store/modules/listOfUsers/thunks.js",[],"/home/joao/atividades/atividades-a-fazer/kenziehub/src/pages/list/style.js",[],"/home/joao/atividades/atividades-a-fazer/kenziehub/src/store/modules/validationToken/thunks.js",[],"/home/joao/atividades/atividad